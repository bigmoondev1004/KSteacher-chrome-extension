{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js" //백그라운드 스크립트 등록
    },
    "permissions": ["storage"], //저장 권한 추가
    "action": {
        "default_popup": "popup.html" //팝업 선언
    }
}